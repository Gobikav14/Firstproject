package conditions;

public class Libraraydays {
	public static void main(String[]args) {
		 int a=3;
		 if(a>=1&&a<=5) {
	 System.out.println(a*2);
		 }
		 else if (a>=6&&a<=10) {
			 System.out.println(a*5);
		 }
		 else if (a>=11&&a<=20) {
			 System.out.println(a*7);
		 }
		 else if (a>=21&&a<=30) {
			 System.out.println(a*8);
		 }
		 else if(a>30) {
			 System.out.println("membership wil be cancelled");
		 }
		 
			 
		 
	}

}

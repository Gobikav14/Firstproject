package For3;

public class Task12 {
	public static void main(String[] args) {
		int a=128;
		int original=a;
		int temp=0;
		while(a>0) {
		int r=a%10;
		temp=temp+(r*r*r);
		a=a/10;
		}
		if(original==temp) {
			System.out.println("Amstrong");
		}
		else {
			System.out.println("Not a amstrong");
		}
		
	}

}

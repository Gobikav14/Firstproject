package For2;

public class StartswithS {
	public static void main(String[] args) {
		String [] x= {"Success","initial","Statements","Manager","Structure"};
		for(int i=0;i<=x.length-1;i++) {
			if(x[i].startsWith("S")&& x[i].endsWith("s")) {
				//x[i].length()/2
			System.out.println(x[i].charAt(x[i].length()/2));
		}
	}
	}
}
